{
  "name": "passb",
  "version": "1.0.0",
  "homepage": "https://passb.github.io",
  "bugs": "https://github.com/passB/passB/issues",
  "description": "passb is a browser addon for the excellent password store command line password manager.",
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "start:firefox": "web-ext run --source-dir ./extension",
    "watch-tests": "jest --watch --verbose",
    "build": "cross-env NODE_ENV=production webpack && web-ext build --source-dir ./extension --overwrite-dest",
    "remotedev": "remotedev --hostname=localhost --port=8000"
  },
  "author": "Lenz Weber",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-mock-store": "^0.0.12",
    "@types/remote-redux-devtools": "0.5.0",
    "awesome-typescript-loader": "^3.3.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.2.2",
    "expose-loader": "^0.7.4",
    "node-sass": "^7.0.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "remote-redux-devtools": "0.5.0",
    "remotedev-server": "^0.2.4",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "web-ext": "^1.10.1",
    "web-ext-types": "^0.3.1",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "@types/deep-extend": "^0.4.30",
    "@types/deep-freeze": "^0.1.1",
    "@types/fuzzaldrin": "^2.1.0",
    "@types/fuzzaldrin-plus": "^0.0.1",
    "@types/jest": "^21.1.4",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lodash.memoize": "^4.1.3",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-redux": "^5.0.11",
    "@types/react-router-dom": "^4.0.7",
    "@types/recompose": "^0.24.1",
    "@types/reflect-metadata": "^0.0.5",
    "@types/reselect": "^2.2.0",
    "deep-extend": "^0.5.0",
    "deep-freeze": "^0.0.1",
    "file-loader": "^0.11.2",
    "fuzzaldrin": "^2.1.0",
    "fuzzaldrin-plus": "^0.4.1",
    "ignore-loader": "^0.1.2",
    "immutable": "4.0.0-rc.9",
    "inversify": "^4.5.0",
    "inversify-inject-decorators": "^3.0.1",
    "jest": "^21.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.15",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-qr-svg": "^2.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "5",
    "redux-persist-transform-immutable": "^4.3.0",
    "reflect-metadata": "^0.1.10",
    "remotedev-serialize": "^0.1.0",
    "scroll-into-view-if-needed": "^1.2.2",
    "ts-jest": "^21.1.3",
    "tslint-react": "^3.2.0",
    "typeface-roboto": "^0.0.35",
    "typescript-fsa": "^2.5.0",
    "typescript-fsa-reducers": "^0.4.4",
    "webextension-polyfill": "^0.2.1"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/__tests__/**",
      "!src/__test_helpers__/**",
      "!**/node_modules/**"
    ],
    "modulePaths": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)",
      "<rootDir>/src/**/?(*.)test.ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest/preprocessor.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/src/__test_helpers__/testSetup.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/__test_helpers__/testFrameworkScriptSetup.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
